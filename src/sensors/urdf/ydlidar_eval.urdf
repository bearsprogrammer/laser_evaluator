<robot name="ydlidarG4">

    <link name="map"/>
    <link name="laser1"/>
    <link name="laser2"/>
    <link name="laser3"/>
    <link name="laser4"/>

    <joint name="map2laser1" type="fixed">
        <parent link="map"/>
        <child link="laser1"/>
        <origin xyz="0 0 0" rpy="0 0 3.14" />
