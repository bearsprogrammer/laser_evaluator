    <joint name="map2laser2" type="fixed">
        <parent link="map"/>
        <child link="laser2"/>
        <origin xyz="0 0 0" rpy="0 0 3.14" />
    </joint>

    <joint name="map2laser3" type="fixed">
        <parent link="map"/>
        <child link="laser3"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>

    <joint name="map2laser4" type="fixed">
        <parent link="map"/>
        <child link="laser4"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>

</robot>
